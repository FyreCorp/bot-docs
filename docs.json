{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "Fyre Bot",
	"favicon": "/public/favicon.png",
	"icons": {
		"library": "lucide"
	},
	"fonts": {
		"family": "Jetbrains Mono"
	},
	"logo": {
		"dark": "/public/icon.png",
		"light": "/public/icon.png"
	},
	"appearance": {
		"strict": true,
		"default": "dark"
	},
	"colors": {
		"dark": "#198BB8",
		"light": "#1FA9E1",
		"primary": "#1FA9E1"
	},
	"seo": {
		"metatags": {
			"theme-color": "#1FA9E1",
			"og:site_name": "Fyre Bot Docs"
		}
	},
	"errors": {
		"404": {
			"title": "You've found a glitch in the matrix",
			"redirect": false,
			"description": "This page doesn't exist yet, or it doesn't exist anymore. [Tap/click here](/) to go back."
		}
	},
	"footer": {
		"socials": {
			"discord": "https://discord.gg/SNxqD4ujTF",
			"github": "https://github.com/FyreCorp"
		}
	},
	"navbar": {
		"primary": {
			"type": "button",
			"label": "Add App",
			"href": "https://discord.com/oauth2/authorize?client_id=1350451983022166158"
		},
		"links": [
			{
				"type": "button",
				"label": "Docs",
				"href": "/docs/syntax"
			}
		]
	},
	"navigation": {
		"global": {
			"anchors": [
				{
					"icon": "square-pen",
					"anchor": "Changelog",
					"href": "/changelog"
				},
				{
					"icon": "discord",
					"anchor": "Discord Server",
					"href": "https://discord.gg/SNxqD4ujTF"
				}
			]
		},
		"groups": [
			{
				"icon": "rocket",
				"group": "The Basics",
				"pages": [
					"docs/syntax",
					"docs/commands",
					"docs/permissions"
				]
			},
			{
				"icon": "scroll",
				"group": "Resources",
				"pages": [
					"docs/guides/vip",
					"docs/guides/config",
					"docs/guides/automation"
				]
			},
			{
				"icon": "settings",
				"group": "Modules",
				"pages": [
					"docs/modules/vip",
					"docs/modules/afk",
					"docs/modules/jail",
					"docs/modules/snipe",
					"docs/modules/forms",
					"docs/modules/last-fm",
					"docs/modules/activity",
					"docs/modules/join-gate",
					"docs/modules/purge",
					"docs/modules/responses",
					"docs/modules/threads",
					"docs/modules/time",
					"docs/modules/backup",
					"docs/modules/media",
					{
						"icon": "tickets",
						"group": "Tickets",
						"pages": ["docs/modules/tickets/general", "docs/modules/tickets/managing"]
					}
				]
			},
			{
				"icon": "message-circle-code",
				"group": "Templating",
				"pages": [
					"docs/templating/syntax",
					"docs/templating/variables",
					"docs/templating/message-templates",
					"docs/templating/defaults"
				]
			},
			{
				"icon": "gavel",
				"group": "Legal",
				"pages": [
					"terms",
					"privacy",
					"orders"
				]
			}
		]
	}
}
