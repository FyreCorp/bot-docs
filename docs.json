{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "Fyre Bot",
	"favicon": "/favicon.png",
	"appearance": {
		"strict": true
	},
	"icons": {
		"library": "lucide"
	},
	"logo": {
		"dark": "/icon.png",
		"light": "/icon.png"
	},
	"colors": {
		"dark": "#198BB8",
		"light": "#1FA9E1",
		"primary": "#1FA9E1"
	},
	"banner": {
		"content": "Fyre Bot is currently in Early Access. Please report any bugs or give any feedback to @thefallenspirit on Discord."
	},
	"navigation": {
		"global": {
			"anchors": [
				{
					"icon": "discord",
					"anchor": "Invite / Install",
					"href": "https://discord.com/oauth2/authorize?client_id=1350451983022166158"
				}
			]
		},
		"groups": [
			{
				"group": "The Basics",
				"pages": ["syntax", "perms/bot"]
			},
			{
				"group": "Modules",
				"pages": [
					"modules/auto-purge",
					"modules/auto-thread",
					{
						"icon": "tickets",
						"group": "Tickets",
						"pages": ["modules/tickets/getting-started", "modules/tickets/options"]
					}
				]
			},
			{
				"group": "Miscellaneous",
				"pages": ["misc/variables"]
			}
		]
	},
	"footer": {
		"socials": {
			"github": "https://github.com/FyreCorp"
		}
	}
}
