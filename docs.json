	"name": "Fyre Bot",
	"favicon": "/favicon.png",
	"icons": {
		"library": "lucide"
	},
	"logo": {
		"dark": "/icon.png",
		"light": "/icon.png"
	},
	"appearance": {
		"strict": true,
		"default": "dark"
	},
	"colors": {
