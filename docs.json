			"theme-color": "#1FA9E1",
			"og:site_name": "Fyre Bot Docs"
		}
	},
	"navigation": {
		"global": {
			"anchors": [
				{
					"icon": "plus",
					"anchor": "Add App",
					"href": "https://discord.com/oauth2/authorize?client_id=1350451983022166158"
				},
				{
					"icon": "circle-help",
					"anchor": "Discord Server",
					"href": "https://discord.gg/SNxqD4ujTF"
				}
			]
		},
		"groups": [
			{
				"group": "The Basics",
				"pages": [
					"basics/syntax",
